<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jrosario.d04052022.spendingtracer">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.SpendingTracer">
        <activity
            android:name=".activities.spendingcategorydetails.SpendingCategoryDetailsActivity"
            android:exported="false" />
        <activity
            android:name=".activities.spendingtracer.SpendingTracerActivity"
            android:exported="false" />
        <activity
            android:name=".activities.splashscreen.MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>